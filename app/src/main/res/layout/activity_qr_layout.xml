<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="#ecedec">    <View        android:id="@+id/view"        android:layout_width="match_parent"        android:layout_height="@dimen/status_bar_height"        android:background="@color/ht_hred_title" />    <com.example.android_projectview02.widget.TitleLayout        android:layout_below="@+id/view"        android:id="@+id/mtitle"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/ht_hred_title"        app:lib_pub_tl_leftDrawable="@drawable/btn_back"        app:lib_pub_tl_middleText="@string/my_qr_code"        app:lib_pub_tl_rightDrawable="@drawable/btn_back" />    <RelativeLayout        android:layout_width="340dp"        android:layout_height="500dp"        android:layout_centerInParent="true"        >        <RelativeLayout            android:id="@+id/slid_currentuser"            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:orientation="horizontal">            <com.example.android_projectview02.widget.RoundImageView                android:id="@+id/imageview_residemenu_avatar"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_margin="20dp"                android:src="@drawable/ic_error_page" />            <TextView                android:id="@+id/default_tv"                android:layout_width="60dp"                android:layout_height="60dp"                android:layout_alignParentLeft="true"                android:layout_centerVertical="true"                android:layout_marginLeft="20dp"                android:background="@drawable/textview_style"                android:gravity="center"                android:textColor="@color/white"                android:textSize="20dip"                android:visibility="visible" />            <TextView                android:id="@+id/textview_currentUserName"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerVertical="true"                android:layout_toRightOf="@+id/imageview_residemenu_avatar"                android:text="测试"                android:textColor="@color/color_333333"                android:textSize="@dimen/fontsize_heading16"                android:textStyle="bold" />        </RelativeLayout>        <ImageView            android:id="@+id/qr_pic"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:layout_centerInParent="true"            android:src="@drawable/xiongm" />        <TextView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_alignParentBottom="true"            android:layout_centerHorizontal="true"            android:layout_marginTop="36dp"            android:layout_marginBottom="80dp"            android:text="@string/my_qr_pic_text"            android:textColor="@color/mx_grey_dark2"            android:textSize="@dimen/fontsize_heading14" />    </RelativeLayout></RelativeLayout>