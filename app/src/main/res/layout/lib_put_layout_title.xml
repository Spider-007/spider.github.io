<?xml version="1.0" encoding="utf-8"?><merge xmlns:android="http://schemas.android.com/apk/res/android">    <ImageView        android:id="@+id/iv_title_left"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentLeft="true"        android:layout_centerVertical="true"        android:padding="14dp"        android:scaleType="centerCrop"        android:visibility="gone" />    <TextView        android:id="@+id/tv_title_left"        android:layout_width="wrap_content"        android:layout_height="@dimen/layout_height"        android:layout_alignParentLeft="true"        android:gravity="center"        android:singleLine="true"        android:textColor="@color/color_393939"        android:textSize="18sp"        android:visibility="gone" />    <ImageView        android:id="@+id/iv_title_middle"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_centerInParent="true"        android:scaleType="centerCrop"        android:visibility="gone" />    <TextView        android:id="@+id/tv_title_title"        android:layout_width="wrap_content"        android:layout_height="@dimen/layout_height"        android:layout_centerInParent="true"        android:ellipsize="middle"        android:gravity="center_vertical"        android:singleLine="true"        android:textColor="@color/color_393939"        android:textSize="18sp"        android:visibility="gone" />    <TextView        android:id="@+id/tv_title_right"        android:layout_width="wrap_content"        android:layout_height="@dimen/layout_height"        android:layout_alignParentRight="true"        android:layout_centerVertical="true"        android:layout_marginRight="12dp"        android:gravity="center"        android:singleLine="true"        android:textColor="#567FE6"        android:textSize="12sp"        android:visibility="gone" />    <ImageView        android:id="@+id/iv_title_right"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_alignParentRight="true"        android:layout_centerVertical="true"        android:layout_marginRight="14dp"        android:scaleType="centerCrop"        android:visibility="gone" /></merge>